def sd(s):
    if ((s[0] * s[1] * s[2])-s[2]) >= d:
        r.append(s[0] * s[1] * s[2])
    else:
        s[2] += d
        sd(s)
    return r


if __name__ == "__main__":

    r = []
    t = int(input())
    for i in range(t):
        s = [1]
        d = int(input())
        i = 0
        while len(s)!=3:
            s.append(int(s[i])+d)
            i+=1
        sd(s)
for i in range(len(r)):
    print(r[i])

