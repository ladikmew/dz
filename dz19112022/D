def chet(n,r):
    n =sorted(n)
    r.append(n[1])

def nechet(n,r):
    n = sorted(n)
    r.append(n[0])


if __name__ =="__main__":
    t = int(input())
    r =[]
    for i in range(t):
        n = input()
        if len(n)==2:
            chet(n,r)
        else:
            nechet(n,r)
for i in range(len(r)):
    print(r[i])
