t = int(input())
s = []
def prostik(a):
    if a < 2:
        return 0
    for i in range(2, int(a ** 0.5 + 1)):
        if a % i == 0:
            return 0
    else:
        return 1

for i in range(t):
    d = int(input())
    delit2 = 1+d

    while prostik(delit2)!=1:
        delit2+=1
    delit3 = delit2+d
    while prostik(delit3)!=1:
        delit3+=1
    s.append(delit2*delit3)

for i in s:
    print(i)

