def marge(a: list, b:list) -> int:
    ai = 0
    bi = 0
    c = []
    while ai<len(a) and bi<len(b):
        if a[ai] <= b[bi]:
            c.append(a[ai])
            ai+=1
        else:
            c.append(b[bi])
            bi+=1
    while ai < len(a):
        c.append(a[ai])
        ai += 1
    while bi < len(b):
        c.append(b[bi])
        bi += 1
    return c
